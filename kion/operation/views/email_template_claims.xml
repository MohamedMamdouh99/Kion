        <!--claims template after _ fix _____________________________-->


        <record id="mail_after_fix_template1dd1" model="mail.template">
            <field name="name">bib Template</field>
            <field name="model_id" ref="model_after_fix"/>
            <field name="email_from">{{object.env.user.work_email}}</field>
            <field name="subject">طلب معاينة بعد الاصلاح</field>
            <field name="auto_delete" eval="True"/>


            <field name="body_html" type="html">
                <div>
                    hello we are bib company and this form about insurance order
                </div>
                <div>ألاسم :
                    <t t-out="object.name"/>
                </div>
                <div>ملاحظات :
                    <t t-out="object.note_sent"/>
                </div>


            </field>
            <field name="report_template" ref="operation.action_report_after_fix"/>
            <field name="report_name">{{ (object.name or '')}}</field>
            <field name="lang">{{ object.choose_partner.lang }}</field>


        </record>










